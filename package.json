{
	"name": "web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "infisical run -- docker compose up -d && infisical run -- vite dev",
		"build": "infisical run -- vite build",
		"preview": "infisical run -- vite preview",
		"prepare": "infisical run -- svelte-kit sync || echo ''",
		"prod": "tsm prodserver.ts",
		"check": "infisical run -- svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "infisical run -- svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "infisical run -- prettier --write .",
		"lint": "infisical run -- prettier --check . && eslint .",
		"test:e2e": "infisical run -- playwright test",
		"test": "infisical run -- npm run test:e2e",
		"db:generate": "infisical run -- drizzle-kit generate",
		"db:push": "infisical run -- drizzle-kit push",
		"db:migrate": "infisical run -- drizzle-kit migrate",
		"db:studio": "infisical run -- drizzle-kit studio",
		"db:reset": "infisical run -- drizzle-kit reset",
		"db:start": "infisical run -- docker compose up",
		"db:stop": "infisical run -- docker compose down",
		"ci:build": "vite build",
		"ci:lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.8",
